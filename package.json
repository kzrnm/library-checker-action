{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Actions that run yosupo06/library-checker-problems",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "lint-staged": "lint-staged",
    "all": "npm run build && npm run format && npm run lint-fix && npm run package && npm test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naminodarie/library-checker-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "naminodarie",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "delay": "^5.0.0",
    "git-clone": "^0.1.0",
    "json-stable-stringify": "^1.0.1",
    "p-cancelable": "^2.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/json-stable-stringify": "^1.0.33",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/parser": "^4.28.2",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
